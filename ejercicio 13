#include <iostream>
#include <cmath>
using namespace std;

double redondear(double numero, int decimales) {
    double factor = pow(10, decimales);
    return round(numero * factor) / factor;
}

int main() {
    double numero;
    int decimales;

    cout << "REDONDEO DE NUMEROS" << endl;
    cout << "Ingrese el numero: ";
    cin >> numero;
    cout << "Ingrese el numero de decimales: ";
    cin >> decimales;

    double resultado = redondear(numero, decimales);
    cout << "Numero redondeado: " << resultado << endl;

    return 0;
}
